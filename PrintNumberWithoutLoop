package com.DemoRest.demotest;

/**
 * Print 1 to 100 without using loops
 * 1. recursive function
 * 2. Java Streams 
 *
 */
public class App 
{
    public static void main( String[] args )
    {
       printnum(1);
    }
    
    public static void printnum(int i){
    	if(i<=100){
    	System.out.println(i);
    		i++;
    		printnum(i);
    	}
    	
    }
}
